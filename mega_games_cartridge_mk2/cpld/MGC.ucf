# 16 MHz clock
NET "CLK16MHz" TNM_NET = CLK16MHz;
TIMESPEC TS_CLK16MHz = PERIOD "CLK16MHz" 16 MHz HIGH 50%;

# 6502 clock
NET "PHI" TNM_NET = PHI;
TIMESPEC TS_PHI = PERIOD "PHI" 2 MHz HIGH 50%;

